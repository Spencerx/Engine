<?xml version="1.0" encoding="utf-8"?>
<CompositionSpecification xmlns="http://netide.eu/schemas/compositionspecification/v1">
  <Modules>
    <Module id="SimpleSwitchA" loaderIdentification="appA.py"/>
    <Module id="SimpleSwitchB" loaderIdentification="appB.py"/>
  </Modules>
  <Composition>
    <ParallelCall resolutionPolicy="priority">
      <ModuleCall module="SimpleSwitchA" priority="1" deadTimeOut="10"/>
      <ModuleCall module="SimpleSwitchB" priority="2" deadTimeOut="10"/>
    </ParallelCall>
  </Composition>
</CompositionSpecification>